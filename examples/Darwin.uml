@startuml
Darwin   *- Creature
Creature o- Species

class Darwin {
    - vector<Creature> _creatures
    - vector<vector<Creature*>> _grid
    + read ()
    + eval ()
    + print ()
    }

class Creature {
    - Species _s
    - enum _d
    - int _pc
    + eval ()
    }

class Species {
    - pair<int, int> _p
    + eval ()
    }

@enduml
